Map<String, String> cdMap = new Map<String, String>();
cdMap.put('1','0695g000000otgeAAA');
cdMap.put('2','0695g000000otgeAAA');
System.debug(cdMap.keySet());
System.debug(cdMap.values());

List<ContentVersion> cvList = [
    							SELECT	Id
    									,Title
                                       	,VersionData
                                       	,FileExtension
    									,ContentDocumentID
								FROM	ContentVersion
                               	WHERE	ContentDocumentID IN :cdMap.values()
                                      ];
List<String, ContentVersion> cvMap = new List<String, ContentVersion>();

for(ContentVersion cv : cvList){
    cvMap.put(cv.ContentDocumentID, cv);
}

List<PDFfileMerge.ReqPDFJson> listFiles = new List<PDFfileMerge.ReqPDFJson>();

for(String seq : cdMap.keySet()){
    ContentVersion cv = cvMap.get(cdMap.get(seq));
    PDFfileMerge.ReqPDFJson	pdfJson = new PDFfileMerge.ReqPDFJson();
    pdfJson.Title = cv.Title; 					// Title
    pdfJson.VersionData = cv.VersionData; 		// VersionData
    pdfJson.FileExtension = cv.FileExtension; 	// FileExtension
    pdfJson.ContentDocumentId = cv.Id; 			// ContentVersion Id
    pdfJson.Seq = seq; 							// 순서
    listFiles.add(pdfJson);
}

System.debug(listFiles);

